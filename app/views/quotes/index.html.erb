<p id="notice"><%= notice %></p>

<h1>Quotes</h1>

<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th>Date</th>
      <th>Open</th>
      <th>High</th>
      <th>Low</th>
      <th>Close</th>
      <th>Adjusted close</th>
      <th>Volume</th>
      <th>Dividend</th>
      <th>Split coefficient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.ticker %></td>
        <td><%= quote.date %></td>
        <td><%= quote.open %></td>
        <td><%= quote.high %></td>
        <td><%= quote.low %></td>
        <td><%= quote.close %></td>
        <td><%= quote.adjusted_close %></td>
        <td><%= quote.volume %></td>
        <td><%= quote.dividend %></td>
        <td><%= quote.split_coefficient %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
